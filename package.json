{
  "name": "hx-api",
  "version": "1.0.0",
  "description": "A Simple node.js api",
  "scripts": {
    "start": "npm-run-all --parallel server:run client:build",
    "server:run": "nodemon --exec babel-node src/api/server.js",
    "client:build": "webpack --mode production --watch",
    "debug": "nodemon --inspect --exec babel-node src/api/server.js",
    "test": "jest",
    "lint": ""
  },
  "author": "Greg Smith",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fastest-validator": "^0.6.12",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "pm2": "^3.2.2",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "verbose": true,
    "watchPathIgnorePatterns": [
      "node_modules",
      "api/db/data"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gregjsmith/hx-api"
  }
}
